# baseURI: http://www.gwax.com/oracle/

@prefix :        <http://www.gwax.com/oracle/> .
@prefix block:   <http://www.gwax.com/oracle/cardblock#> .
@prefix card:    <http://www.gwax.com/oracle/cardname#> .
@prefix magiccardsinfo:  <http://magiccards.info/> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix print:   <http://www.gwax.com/oracle/cardprint#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix set:     <http://www.gwax.com/oracle/cardset#> .
@prefix subtype:  <http://www.gwax.com/oracle/subtype#> .
@prefix supertype:  <http://www.gwax.com/oracle/supertype#> .
@prefix type:    <http://www.gwax.com/oracle/cardtype#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .

magiccardsinfo:enlink
      rdf:type owl:DatatypeProperty ;
      rdfs:range xsd:string .

magiccardsinfo:setcode
      rdf:type owl:DatatypeProperty ;
      rdfs:domain :Set ;
      rdfs:range xsd:string .

:     rdf:type owl:Ontology ;
      owl:versionInfo "Created by George Waksman"^^xsd:string .

:Block
      rdf:type owl:Class ;
      rdfs:subClassOf owl:Thing ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:cardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :name
              ] .

:Card
      rdf:type owl:Class ;
      rdfs:subClassOf owl:Thing ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:minCardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :Type
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:cardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :name
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:cardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :rules
              ] .

:CardPrinting
      rdf:type owl:Class ;
      rdfs:subClassOf owl:Thing ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:cardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :artist
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:cardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :card
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :flavor
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:cardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :set
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:cardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :setNumber
              ] .

:CardType
      rdf:type owl:Class ;
      rdfs:subClassOf :Type .

:Creature
      rdf:type owl:Class ;
      rdfs:subClassOf :Card ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:cardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :power
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:cardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :toughness
              ] .

:DoubleFacedCard
      rdf:type owl:Class ;
      rdfs:subClassOf :DualCard ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:cardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :frontCard
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:cardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :rearCard
              ] .

:DoubleFacedCardPrinting
      rdf:type owl:Class ;
      rdfs:subClassOf :DualCardPrinting ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :frontFlavor
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :rearFlavor
              ] .

:DualCard
      rdf:type owl:Class ;
      rdfs:subClassOf owl:Thing ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:cardinality "2"^^xsd:nonNegativeInteger ;
                owl:onProperty :card
              ] .

:DualCardPrinting
      rdf:type owl:Class ;
      rdfs:subClassOf owl:Thing ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:cardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :artist
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:cardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :dualCard
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:maxCardinality "2"^^xsd:nonNegativeInteger ;
                owl:onProperty :flavor
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:cardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :set
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:cardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :setNumber
              ] .

:FlipCard
      rdf:type owl:Class ;
      rdfs:subClassOf :DualCard ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:cardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :rightsideupCard
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:cardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :upsidedownCard
              ] .

:FlipCardPrinting
      rdf:type owl:Class ;
      rdfs:subClassOf :DualCardPrinting ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:cardinality "0"^^xsd:nonNegativeInteger ;
                owl:onProperty :flavor
              ] .

:Planeswalker
      rdf:type owl:Class ;
      rdfs:subClassOf :Card ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:cardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :loyalty
              ] .

:Set  rdf:type owl:Class ;
      rdfs:subClassOf owl:Thing ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :block
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:cardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :name
              ] .

:SplitCard
      rdf:type owl:Class ;
      rdfs:subClassOf :DualCard ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:cardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :leftCard
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:cardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :rightCard
              ] .

:SplitCardPrinting
      rdf:type owl:Class ;
      rdfs:subClassOf :DualCardPrinting ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :leftFlavor
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :rightFlavor
              ] .

:Subtype
      rdf:type owl:Class ;
      rdfs:subClassOf :Type .

:Supertype
      rdf:type owl:Class ;
      rdfs:subClassOf :Type .

:Type
      rdf:type owl:Class ;
      rdfs:subClassOf owl:Thing ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:cardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :identifier
              ] .

:artist
      rdf:type owl:DatatypeProperty ;
      rdfs:domain :CardPrinting ;
      rdfs:range xsd:string .

:block
      rdf:type owl:ObjectProperty ;
      rdfs:range :Block .

:card
      rdf:type owl:ObjectProperty ;
      rdfs:range :Card .

type:Artifact
      rdf:type :CardType ;
      :identifier "Artifact"^^xsd:string .

type:Creature
      rdf:type :CardType ;
      :identifier "Creature"^^xsd:string .

type:Enchantment
      rdf:type :CardType ;
      :identifier "Enchantment"^^xsd:string .

type:Instant
      rdf:type :CardType ;
      :identifier "Instant"^^xsd:string .

type:Land
      rdf:type :CardType ;
      :identifier "Land"^^xsd:string .

type:Phenomenon
      rdf:type :CardType ;
      :identifier "Phenomenon"^^xsd:string .

type:Plane
      rdf:type :CardType ;
      :identifier "Plane"^^xsd:string .

type:Planeswalker
      rdf:type :CardType ;
      :identifier "Planeswalker"^^xsd:string .

type:Scheme
      rdf:type :CardType ;
      :identifier "Scheme"^^xsd:string .

type:Sorcery
      rdf:type :CardType ;
      :identifier "Sorcery"^^xsd:string .

type:Tribal
      rdf:type :CardType ;
      :identifier "Tribal"^^xsd:string .

type:Vanguard
      rdf:type :CardType ;
      :identifier "Vanguard"^^xsd:string .

:cmc  rdf:type owl:DatatypeProperty ;
      rdfs:range xsd:integer .

:cost
      rdf:type owl:DatatypeProperty ;
      rdfs:range xsd:string .

:dualCard
      rdf:type owl:ObjectProperty ;
      rdfs:range :DualCard .

:flavor
      rdf:type owl:DatatypeProperty ;
      rdfs:domain :CardPrinting ;
      rdfs:range xsd:string .

:frontCard
      rdf:type owl:ObjectProperty ;
      rdfs:domain :DoubleFacedCard ;
      rdfs:subPropertyOf :card .

:frontFlavor
      rdf:type owl:DatatypeProperty ;
      rdfs:domain :DoubleFacedCardPrinting ;
      rdfs:subPropertyOf :flavor .

:identifier
      rdf:type owl:DatatypeProperty ;
      rdfs:range xsd:string .

:leftCard
      rdf:type owl:ObjectProperty ;
      rdfs:domain :SplitCard ;
      rdfs:subPropertyOf :card .

:leftFlavor
      rdf:type owl:DatatypeProperty ;
      rdfs:domain :SplitCardPrinting ;
      rdfs:subPropertyOf :flavor .

:loyalty
      rdf:type owl:DatatypeProperty ;
      rdfs:domain :Planeswalker ;
      rdfs:range xsd:integer .

:name
      rdf:type owl:DatatypeProperty ;
      rdfs:range xsd:string ;
      rdfs:subPropertyOf :identifier .

:power
      rdf:type owl:DatatypeProperty ;
      rdfs:domain :Creature ;
      rdfs:range
              [ rdf:type owl:Class ;
                owl:unionOf (xsd:integer xsd:string)
              ] .

:rearCard
      rdf:type owl:ObjectProperty ;
      rdfs:domain :DoubleFacedCard ;
      rdfs:subPropertyOf :card .

:rearFlavor
      rdf:type owl:DatatypeProperty ;
      rdfs:domain :DoubleFacedCardPrinting ;
      rdfs:subPropertyOf :flavor .

:rightCard
      rdf:type owl:ObjectProperty ;
      rdfs:domain :SplitCard ;
      rdfs:subPropertyOf :card .

:rightFlavor
      rdf:type owl:DatatypeProperty ;
      rdfs:domain :SplitCardPrinting ;
      rdfs:subPropertyOf :flavor .

:rightsideupCard
      rdf:type owl:ObjectProperty ;
      rdfs:domain :FlipCard ;
      rdfs:subPropertyOf :card .

:rules
      rdf:type owl:ObjectProperty ;
      rdfs:range rdf:Bag .

:set  rdf:type owl:ObjectProperty ;
      rdfs:range :Set .

:setNumber
      rdf:type owl:DatatypeProperty ;
      rdfs:range xsd:integer .

supertype:Basic
      rdf:type :Supertype ;
      :identifier "Basic"^^xsd:string .

supertype:Legendary
      rdf:type :Supertype ;
      :identifier "Legendary"^^xsd:string .

supertype:Ongoing
      rdf:type :Supertype ;
      :identifier "Ongoing"^^xsd:string .

supertype:Snow
      rdf:type :Supertype ;
      :identifier "Snow"^^xsd:string .

supertype:World
      rdf:type :Supertype ;
      :identifier "World"^^xsd:string .

:toughness
      rdf:type owl:DatatypeProperty ;
      rdfs:domain :Creature ;
      rdfs:range
              [ rdf:type owl:Class ;
                owl:unionOf (xsd:integer xsd:string)
              ] .

:type
      rdf:type owl:ObjectProperty ;
      rdfs:domain :Card ;
      rdfs:range :Type .

:upsidedownCard
      rdf:type owl:ObjectProperty ;
      rdfs:domain :FlipCard ;
      rdfs:subPropertyOf :card .
