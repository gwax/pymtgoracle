:cmc  rdf:type owl:DatatypeProperty ;
      rdfs:range xsd:integer .

:cost
      rdf:type owl:DatatypeProperty ;
      rdfs:range xsd:string .

:flavor
      rdf:type owl:DatatypeProperty , owl:ObjectProperty ;
      rdfs:domain :CardPrinting ;
      rdfs:range
              [ rdf:type owl:Class ;
                owl:unionOf (xsd:string rdf:Seq owl:Nothing)
              ] .

:identifier
      rdf:type owl:DatatypeProperty ;
      rdfs:range xsd:string .

:loyalty
      rdf:type owl:DatatypeProperty , owl:ObjectProperty ;
      rdfs:domain :Planeswalker ;
      rdfs:range xsd:integer .

:name
      rdf:type owl:DatatypeProperty ;
      rdfs:range xsd:string ;
      rdfs:subPropertyOf :identifier .

:power
      rdf:type owl:DatatypeProperty ;
      rdfs:domain :Creature ;
      rdfs:range
              [ rdf:type owl:Class ;
                owl:unionOf (xsd:integer xsd:string)
              ] .

:rarity
      rdf:type owl:ObjectProperty ;
      rdfs:range :Rarity .

:rules
      rdf:type owl:ObjectProperty ;
      rdfs:range
              [ rdf:type owl:Class ;
                owl:unionOf (rdf:Seq owl:Nothing)
              ] .

:set  rdf:type owl:ObjectProperty ;
      rdfs:range :Set .

:setNumber
      rdf:type owl:DatatypeProperty ;
      rdfs:range xsd:integer .

:subtype
