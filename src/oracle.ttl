type:Artifact
      rdf:type :CardType ;
      rdfs:label "Artifact"^^xsd:string ;
      :identifier "Artifact"^^xsd:string .

type:Creature
      rdf:type :CardType ;
      rdfs:label "Creature"^^xsd:string ;
      :identifier "Creature"^^xsd:string .

type:Enchantment
      rdf:type :CardType ;
      rdfs:label "Enchantment"^^xsd:string ;
      :identifier "Enchantment"^^xsd:string .

