rarity:Special
      rdf:type :Rarity ;
      rdfs:label "Special"^^xsd:string ;
      :identifier "Special"^^xsd:string .

rarity:Uncommon
      rdf:type :Rarity ;
      rdfs:label "Uncommon"^^xsd:string ;
      :identifier "Uncommon"^^xsd:string .

<http://www.gwax.com/oracle/cardrarity#Uncommon_%28C1%29>
      rdf:type :Rarity ;
