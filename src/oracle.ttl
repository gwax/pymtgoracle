rarity:Rare
      rdf:type :Rarity ;
      rdfs:label "Rare"^^xsd:string ;
      :identifier "Rare"^^xsd:string .

<http://www.gwax.com/oracle/cardrarity#Rare_%28C3%29>
      rdf:type :Rarity ;
