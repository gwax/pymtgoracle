# baseURI: http://www.gwax.com/oracle/

@prefix :        <http://www.gwax.com/oracle/> .
@prefix block:   <http://www.gwax.com/oracle/cardblock#> .
@prefix card:    <http://www.gwax.com/oracle/cardname#> .
@prefix magiccardsinfo:  <http://www.magiccards.info/> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix rarity:  <http://www.gwax.com/oracle/cardrarity#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix set:     <http://www.gwax.com/oracle/cardset/> .
@prefix subtype:  <http://www.gwax.com/oracle/subtype#> .
@prefix supertype:  <http://www.gwax.com/oracle/supertype#> .
@prefix type:    <http://www.gwax.com/oracle/cardtype#> .
@prefix xml:     <http://www.w3.org/XML/1998/namespace> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .

:     rdf:type owl:Ontology ;
      owl:versionInfo "Created by George Waksman"^^xsd:string .

:Artifact
      rdf:type owl:Class ;
      rdfs:label "Artifact"^^xsd:string ;
      rdfs:subClassOf :Card ;
      owl:equivalentClass
              [ rdf:type owl:Restriction ;
                owl:hasValue type:Artifact ;
                owl:onProperty :type
              ] .

:Block
      rdf:type owl:Class ;
      rdfs:subClassOf owl:Thing ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:cardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :name
              ] .

:Card
      rdf:type owl:Class ;
      rdfs:subClassOf owl:Thing ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:cardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :cmc
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:cardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :name
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:cardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :rules
              ] .

:CardPrinting
      rdf:type owl:Class ;
      rdfs:subClassOf owl:Thing ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:cardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :artist
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:cardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :card
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:cardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :flavor
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:cardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :rarity
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:cardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :set
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:cardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :setNumber
              ] .

:CardType
      rdf:type owl:Class ;
      rdfs:subClassOf :Type .

:Creature
      rdf:type owl:Class ;
      rdfs:subClassOf :Card ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:cardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :power
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:allValuesFrom :CreatureType ;
                owl:onProperty :subtype
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:cardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :toughness
              ] ;
      owl:equivalentClass
              [ rdf:type owl:Restriction ;
                owl:hasValue type:Creature ;
                owl:onProperty :type
              ] .

:CreatureType
      rdf:type owl:Class ;
      rdfs:label "Creature type"^^xsd:string ;
      rdfs:subClassOf :Subtype .

:Land
      rdf:type owl:Class ;
      rdfs:label "Land"^^xsd:string ;
      rdfs:subClassOf :Card ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:cardinality "0"^^xsd:nonNegativeInteger ;
                owl:onProperty :cost
              ] ;
      owl:equivalentClass
              [ rdf:type owl:Restriction ;
                owl:hasValue type:Land ;
                owl:onProperty :type
              ] .

:Planeswalker
      rdf:type owl:Class ;
      rdfs:subClassOf :Card ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:cardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :loyalty
              ] ;
      owl:equivalentClass
              [ rdf:type owl:Restriction ;
                owl:hasValue type:Planeswalker ;
                owl:onProperty :type
              ] .

:Rarity
      rdf:type owl:Class ;
      rdfs:subClassOf owl:Thing ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:cardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :identifier
              ] .

:Set  rdf:type owl:Class ;
      rdfs:subClassOf rdf:Seq , owl:Thing ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :block
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:cardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :name
              ] .

:Subtype
      rdf:type owl:Class ;
      rdfs:subClassOf :Type .

:Supertype
      rdf:type owl:Class ;
      rdfs:subClassOf :Type .

:Type
      rdf:type owl:Class ;
      rdfs:subClassOf owl:Thing ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:cardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :identifier
              ] .

:artist
      rdf:type owl:DatatypeProperty ;
      rdfs:domain :CardPrinting ;
      rdfs:range xsd:string .

:block
      rdf:type owl:ObjectProperty ;
      rdfs:range :Block .

:card
      rdf:type owl:ObjectProperty ;
      rdfs:range
              [ rdf:type owl:Class ;
                owl:unionOf (:Card rdf:Seq)
              ] .

